{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "src/*": [
        "src/*"
      ],
      "src/domain": [
        "src/domain/src/index"
      ],
      "src/domain/entities": [
        "src/domain/src/entities"
      ],
      "src/domain/entities/*": [
        "src/domain/src/entities/*"
      ],
      "src/domain/flows": [
        "src/domain/src/flows"
      ],
      "src/domain/flows/*": [
        "src/domain/src/flows/*"
      ]
    }
  }
}
